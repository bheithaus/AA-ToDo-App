<%= form_for(@project) do |f| %>
<ul>
	<% @project.errors.full_messages.each do |error|%>
		<li><%= error %></li>
	<% end %>
</ul>
	<%= f.label :title %><br>
	<%= f.text_field :title %>

	<br>
	<%= f.label :description %><br>
	<%= f.text_area :description %>
	
	<br>
	
	<label>
		Team
		<%= f.collection_select :team_id, Team.all, :id, :name, :include_blank => true %>
	</label>
	<%= f.submit %>	
<% end %>